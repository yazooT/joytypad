<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane gridLinesVisible="true" prefHeight="150.0" prefWidth="150.0"
          xmlns="http://javafx.com/javafx/8.0.172-ea"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="joytypad.Controller">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
    <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
        <Label fx:id="one" text="A" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <Label fx:id="two" text="A" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Label fx:id="three" text="A" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Label fx:id="four" text="A" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <Label fx:id="five" text="A" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
</GridPane>
